{% extends "BabySitterBundle::layout.html.twig" %}
{% block evaluation %}
<form method="POST">
    <h2>Evaluation BabySitter</h2>
   <table>
       <tr>
            <td>Nom Babysitter  : </td>
            <td>{{ form_widget(form.iduser) }}</td>
       </tr>
        <tr>
            <td>Note  : </td>
            <td>{{ form_widget(form.note) }}</td>
        </tr>
        <tr>
            <td>Date  : </td>
            <td>{{ form_widget(form.date) }}</td>
        </tr>
        <tr>
            <td>Description : </td>
            <td>{{ form_widget(form.description) }} </td>
        </tr>  
        
        <tr><td> <input type="submit" value="Ajouter" name="ajouter"/></td></tr>
   </table>
         {{ form_rest(form) }} 
</form>
{% endblock %}