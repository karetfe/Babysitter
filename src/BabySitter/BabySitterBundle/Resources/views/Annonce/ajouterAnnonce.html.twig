{# empty Twig template #}
<form method="POST">
<h2>Ajout Annonce </h2>
    <table>
        <tr>
            <td>Titre : </td>
            <td>{{ form_widget(form.titre_annonce) }}</td>
        </tr>
        <tr>
            <td>Description : </td>
            <td>{{ form_widget(form.description) }}</td>
        </tr>
        <tr>
            <td>Disponibilite : </td>
            <td>{{ form_widget(form.disponibilite) }}</td>
        </tr>
       <tr>
            <td>    <input type="submit" name="submit" value="Ajouter"/> </td>
            <td></td>
        </tr>
    </table>
       {{ form_rest(form) }}  
</form> 